AC_PREREQ(2.52)
AC_INIT(LargePrint)

AM_CONFIG_HEADER(config.h)

AM_INIT_AUTOMAKE(gnome-themes, 2.8.0)

# Honor aclocal flags
ACLOCAL="$ACLOCAL $ACLOCAL_FLAGS"


GETTEXT_PACKAGE=gnome-themes
AC_SUBST(GETTEXT_PACKAGE)

AM_MAINTAINER_MODE

AC_PROG_INTLTOOL([0.23])
AC_PROG_CC
AM_PROG_LIBTOOL


PKG_CHECK_MODULES(GTKTHEME,
		  gtk+-2.0 >= 1.3.11
		  gdk-pixbuf-2.0 >= 1.3.11
		  gtk-engines-2)

AC_SUBST(GTKTHEME_CFLAGS)
AC_SUBST(GTKTHEME_LIBS)

GTK_BINARY_VERSION=`$PKG_CONFIG --variable=gtk_binary_version gtk+-2.0`
AC_SUBST(GTK_BINARY_VERSION)

# Checks for header files.
AC_HEADER_STDC

# Checks for typedefs, structures, and compiler characteristics.
AC_C_CONST

# Checks for endianness (needed by GdkRgb).
AC_C_BIGENDIAN

# Checks for library functions.
AC_TYPE_SIGNAL
AC_FUNC_MMAP
AC_FUNC_ALLOCA
AC_CHECK_FUNCS(setenv putenv)

AC_ARG_ENABLE(all-themes,
 [  --enable-all-themes  Install all a11y theme sets, default is core a11y themes only ],,
 all_themes=no)
AM_CONDITIONAL(CORE_THEMES_ONLY, test "$all_themes" = no)

# Theme engines
PKG_CHECK_MODULES(THEME_ENGINE, gtk+-2.0 >= 2.0.0,,
				 AC_MSG_ERROR([GTK+-2.0 is required to compile theme engines]))
AC_SUBST(THEME_ENGINE_CFLAGS)
AC_SUBST(THEME_ENGINE_LIBS)


GTK_VERSION=`$PKG_CONFIG --variable=gtk_binary_version gtk+-2.0`
AC_SUBST(GTK_VERSION)

### Icon sizes for all large print themes


ALL_LINGUAS="am ar az be bg bn bs ca cs cy da de el en_CA en_GB es et eu fa fi fr ga gu he hi hr hu id is it ja ko li lt lv mk ml mn ms nb nl nn no pa pl pt pt_BR ro ru sk sl sq sr sr@Latn sv ta th tr uk vi wa zh_CN zh_TW"
AM_GLIB_GNU_GETTEXT

LARGEPRINT_DND="48,48"
LARGEPRINT_MENU="32,32"
LARGEPRINT_PANEL_MENU="48,48"
LARGEPRINT_LARGE_TOOLBAR="48,48"
LARGEPRINT_SMALL_TOOLBAR="32,32"
LARGEPRINT_BUTTON="32,32"
LARGEPRINT_DIALOG="64,64"
LARGEPRINT_MINI_COMMANDER="24,24"

LARGEPRINT_ICON_SIZES="mini-commander-icon=$LARGEPRINT_MINI_COMMANDER:print-manager=$LARGEPRINT_DIALOG:panel-button=$LARGEPRINT_BUTTON:gtk-dnd=$LARGEPRINT_DND:gtk-menu=$LARGEPRINT_MENU:panel-menu=$LARGEPRINT_PANEL_MENU:gtk-large-toolbar=$LARGEPRINT_LARGE_TOOLBAR:gtk-small-toolbar=$LARGEPRINT_SMALL_TOOLBAR:gtk-button=$LARGEPRINT_BUTTON:gtk-dialog=$LARGEPRINT_DIALOG"

LARGEPRINT_FONT="sans 18"


AC_SUBST(prefix)
AC_SUBST(LARGEPRINT_ICON_SIZES)
AC_SUBST(LARGEPRINT_FONT)

