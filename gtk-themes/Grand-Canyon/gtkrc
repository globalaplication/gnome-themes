	stretch		= TRUE
      }
    image 
      {
        function        = SHADOW
	recolorable     = TRUE
	shadow          = IN
	file            = "in.png"
	border          = { 2,2,2,2 }
	stretch         = TRUE
      }
    image
      {
	function	= ARROW
	recolorable	= TRUE
	state		= NORMAL
	overlay_file	= "plus.png"
	overlay_border	= { 0, 0, 0, 0 }
